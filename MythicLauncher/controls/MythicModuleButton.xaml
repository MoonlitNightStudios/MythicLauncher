<UserControl x:Class="MythicLauncher.Controls.MythicModuleButton"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MythicLauncher.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="60" d:DesignWidth="60">
    <Grid>
        <Button Background="White" BorderBrush="#FF0010FF" Click="OnClicked">
            <Image x:Name="ModuleIcon" Width="60" Height="60" Source="/Images/Logo_Large.jpg"/>
        </Button>

    </Grid>
</UserControl>

<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
    <ApplicationIcon>images\Logo_Large.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Images\Logo_Large.jpg" />
    <None Remove="Images\Logo_Wide.jpg" />
    <None Remove="Images\Logo_WithBackground.jpg" />
    <None Remove="Images\MoonlitNight.gif" />
    <None Remove="Images\MoonlitNight.png" />
    <None Remove="MoonlitNight.png" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Images\Logo_Large.ico" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Images\Logo_Large.jpg" />
    <Resource Include="Images\Logo_Wide.jpg" />
    <Resource Include="Images\Logo_WithBackground.jpg" />
    <Resource Include="Images\MoonlitNight.gif" />
    <Resource Include="Images\MoonlitNight.png" />
    <Resource Include="MoonlitNight.png" />
  </ItemGroup>

</Project>
